{
	"name": "Nice New Tab",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "A nice new tab",
	"homepage_url": "https://github.com/OiYouYeahYou/new-tab",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": ["src/bg/background.js"],
		"persistent": false
	},
	"chrome_url_overrides": {
		"newtab": "src/override/override.html"
	},
	"permissions": [
		"management",
		"tabs",
		"https://source.unsplash.com/*",
		"https://api.unsplash.com/*"
	]
}
